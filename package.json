{
  "name": "betahub-video-events-sync",
  "version": "1.2.0",
  "type": "module",
  "source": "src/bhves.ts",
  "main": "dist/bhves.js",
  "module": "dist/bhves.esm.js",
  "types": "dist/bhves.d.ts",
  "scripts": {
    "build": "tsc && microbundle -f iife,es",
    "watch": "microbundle watch -f iife,es",
    "dev:example": "cd examples/react && yarn dev",
