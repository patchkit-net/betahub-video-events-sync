    "install:all": "yarn install && cd examples/react && yarn install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "betahub",
    "video",
    "sync",
    "timeline",
    "events"
  ],
  "author": "Upsoft sp. z o.o.",
