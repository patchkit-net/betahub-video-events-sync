  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "concurrently": "^9.1.2",
    "jest": "^30.0.0",
    "microbundle": "^0.15.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.4.2"
  }
}
